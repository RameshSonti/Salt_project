package com.dal.multithreading.pojo;

public class Student {

	private int id;
	private String name;
	private String course;
	private int fee;
	
	public Student() {
		// TODO Auto-generated constructor stub
	}

	public Student(int id, String name, String course, int fee) {
		super();
		this.id = id;
		this.name = name;
		this.course = course;
		this.fee = fee;
	}
	
}
